<phpunit
        enforceTimeLimit="true"
        timeoutForSmallTests="1"
        timeoutForMediumTests="10"
        bootstrap="test-config.php"
        colors="true"
        verbose="true"
        testdox="true"
>
    <testsuites>
        <testsuite name="test package">
            <directory>tests</directory>
        </testsuite>
    </testsuites>
</phpunit>
